        //- button.report-button
        //- a.report-button(href='/api/weeklyrevenue') View
        a#stat.btn.btn-primary(style='margin-left: 100px; margin-top: 10px; background-color:; color:#fff; font-size:16px; font-weight:bold;'  href="/api/weeklyrevenue") View Statistics
      #mid-low.container-fluid
        .row
          .col-md-4.col-sm-12.card1
            i#car-icon.fa-solid.fa-car
