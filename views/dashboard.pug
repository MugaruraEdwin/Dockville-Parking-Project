.divides
  .dashboard
    .menu-btn
      i.fas.fa-bars
    .side-bar
      header
        .close-btn
          i.fas.fa-times
        //- img(src='/images/Cv picture.jpeg' alt='')
        h1 Dockville Parking
      .menu
        .item
          a
            i.fas.fa-desktop
            | Dashboard
        .item
          a.sub-btn
            i.fas.fa-table
            | Battery Section
            // Dropdown
            // Dropdwon arrow
            i.fas.fa-angle-right.dropdown
          .sub-menu
            a.sub-item(href='batteryservices') Battery Hire
            a.sub-item(href='batterysignout') Battery Signout
            // <a href="" class="sub-item"></a>
        .item
          a.sub-btn
            i.fas.fa-th
            | Parking
            // Dropdown
            // Dropdwon arrow
            i.fas.fa-angle-right.dropdown
          .sub-menu
            a.sub-item(href='register') Park Register
            a.sub-item(href='signout') Park Signout
            a.sub-item(href='')
        .item
          a.sub-btn
            i.fas.fa-cog
            | Tyre Section
            // Dropdown
            // Dropdwon arrow
            i.fas.fa-angle-right.dropdown
          .sub-menu
            a.sub-item(href='tyreclinic') Tyre Clinic
            a.sub-item(href='tyreservices') Tyre Services
            a.sub-item(href='')
        .item
          a(href='')
            i.fas.fa-info-circle
