        .item
          a#police.bg-primary(href='/api/logout', style='font-weight:bold; color:white; font-size:18px; border: 1px groove; margin-left:20px; margin-right:130px; padding-left:10px; padding-top:0px; padding-bottom:0px; border-radius:5px;' onclick='return Confirm(event)') Logout


  .divider2
      #welcome.container-fluid
        .row
          #left.col-sm-6
            h4 Welcome Back Manager!
          //- #right.col-sm-6
          //-   i#user.fa-solid.fa-user
          //-   h4.top(style='font-weight:bold;') Manager #{inUser}
